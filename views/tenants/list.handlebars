<h1>Tenants</h1>
<a href="/tenants/add">Add New Tenant</a>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each tenants}}
      <tr>
        <td>{{id}}</td>
        <td>{{first_name}} {{last_name}}</td>
        <td>{{email}}</td>
        <td>{{phone}}</td>
        <td><a href="/tenants/edit/{{id}}">Edit</a></td>
      </tr>
    {{/each}}
  </tbody>
</table>